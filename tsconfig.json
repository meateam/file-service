{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*",
                "src/types/*"
            ]
        },
        "lib": [
            "ES2020",
            "ES2020.Promise",
            "dom"
        ],
    },
    "include": [
        "src/**/*",
        "unrelevent/files.rpc.ts"
    ]
}